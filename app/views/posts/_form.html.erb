<%= form_for(@post) do |f| %>
    <div>
        <%= f.label :title, class: "post-title-label"%>
        <%= f.text_field :title, class: "post-title" %>
    </div>
    
    <div>
        <%= f.label :body, class: "post-body-label" %>
        <%= f.text_field :body, class:"quill_container" %>
    </div>
    
    <%= f.submit class: "btn btn-primary" %>
<% end %>